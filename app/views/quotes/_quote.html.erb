<%= turbo_frame_tag dom_id(quote), class:'p-4 rounded-xl justify-between bg-gray-200 w-96 m-4 flex gap-8' do %>
  <div class="w-full flex justify-between">
    <%= quote.content %>
    <div class="flex">
      <%= link_to 'Edit', edit_quote_path(quote) %>
      <%= button_to "Delete", quote_path(quote),method: :delete %>
    </div>
  </div>
<% end %>